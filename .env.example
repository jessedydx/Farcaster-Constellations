# Environment Variables (Güvenli Versiyon)

# Neynar API (Farcaster data için)
NEYNAR_API_KEY=your_neynar_api_key_here

# Base Network RPC
BASE_RPC_URL=https://mainnet.base.org

# NFT Contract Address (Remix'te deploy ettikten sonra doldur)
NFT_CONTRACT_ADDRESS=

# Pinata (IPFS için)
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_here

# Vercel KV (Database - Vercel deployment sonrası otomatik gelecek)
# Bu değerler Vercel dashboard'dan environment variables'a eklenince otomatik doldurulur
KV_URL=
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=
